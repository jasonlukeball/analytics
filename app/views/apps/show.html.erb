<%= render partial: 'apps/app_header' %>
<%= render partial: 'apps/app_subnav' %>


<div class="app-details">
  <h1><%= @app.name %></h1>
  <h4>url: <%= @app.url %></h4>
  <h4>id_code: <%= @app.id_code %></h4>
  <%= @app.inspect %>
  <br>
  <h4>Events</h4>

  <ul>
    <% @events.each do |key, value| %>
        <li><%= "#{key}: #{value.count}" %></li>
    <% end %>
  </ul>
</div>